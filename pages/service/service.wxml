<!--pages/waiter/waiter.wxml-->
<view class='height320 backC padding2020 margin-bottom80'>
  <view class='back box-shadow height300 border-radius6 waiter flex1'>
      <view class='width-150 height150'>
        <image src='/images/suibe2.jpg'></image>
      </view>
      <view class='waiterTitle margin-top20'>IssueStore</view>
  </view>
</view>
<view class='padding2020'>
  
  <!-- <view class='back online flex-between padding2020' bindtap='clickOpinion2'> -->
      <view class='back tell flex-between padding2020' bindtap="getAllIssue">
    <view class='waiterTitle2 flex-start'>
      <image src='/images/vi.png' class='waiterImg margin-right20'></image>
      <view>查看所有Issues</view>
    </view>
    <view><image src='/images/fangxiang.png' class='jiantou'></image></view>
  </view>
   <view class='back tell flex-between padding2020' bindtap="returns">
    <view class='waiterTitle2 flex-start'>
      <image src='/images/exit.jpg' class='waiterImg margin-right20'></image>
      <view>退出</view>
    </view>
    <view><image src='/images/fangxiang.png' class='jiantou'></image></view>
  </view>
</view>
